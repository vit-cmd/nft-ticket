  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.33.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "@graphprotocol/hardhat-graph": "https://github.com/graphprotocol/hardhat-graph",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "create-next-app": "^13.0.2",
    "hardhat": "^2.12.2"
  },
  "dependencies": {
    "@apollo/client": "^3.7.1",
